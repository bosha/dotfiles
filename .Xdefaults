Xcursor*theme:   jaguarx_twirly

! XFONT SETTINGS

!Xft.dpi:        96
!Xft.antialias:  false
!Xft.rgba:       rgb
!Xft.hinting:    false
!Xft.hintstyle:  hintslight

! URXVT SETTINGS

urxvt.scrollBar: false
urxvt.cursorColor2: #000000
! URxvt*font:   xft:Terminus:size=9
!!!URxvt*font:    xft:DeJaVu Sans Mono:style=Regular:size=9
URxvt*font:     xft:Monaco:size=8:style=Regular
! URxvt.font:   xft:Liberation Mono:pixelsize=12:style=Regular
! URxvt*font:   xft:DejaVu Sans Mono:size=9
! URxvt*font:   xft:Droid Sans Mono:size=9:style=Regular
urxvt*background: #212121
URxvt*background: [50]#000000
urxvt*foreground: #b8b8b8
urxvt*loginShell: true
urxvt*saveLines: 2000
urxvt*urlLauncher:        /usr/bin/firefox
urxvt*boldMode: false
!URxvt*inheritPixmap: true
!URxvt.transparent: true
URxvt*borderLess: false

! teh transparency stuff
! Rxvt.buffered:                         true
! URxvt.background:                       black
! URxvt.foreground:                       #E3E3E3
! URxvt.cursorColor:                      #CCCCCC
! URxvt.transparent:                      0
! URxvt*inheritPixmap:                    true
! URxvt*tintColor:                        white
! URxvt*shading:                          15


URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:	firefox
URxvt.matcher.button:	1
URxvt.colorUL: 		#86a2be

URxvt.perl-lib: /home/bosha/bin/
URxvt.perl-ext: mark-yank-urls
URxvt.keysym.C-i: perl:mark-yank-urls:activate_mark_mode
!URxvt.matcher.rend.0: fgX
!URxvt.urlLauncher: urlopen

!
URxvt*scrollTtyOutput: False
URxvt*scrollWithBuffer: True
URxvt*skipScroll: False

